{
    "name": "YoutProc",
    "version": "1",
    "description": "Youtube Procrastination Killer",
    "permissions": [
        "storage",
        "activeTab",
        "tabs"
    ],
    "background": {
        "scripts": [
            "background.js"
        ],
        "persistent": false
    },
    "options_page": "options.html",
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2
}